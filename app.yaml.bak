application: ideascanvas
version: 1
runtime: python27
api_version: 1
threadsafe: yes

handlers:

- url: /canvasGen
  static_dir: canvasGen
  
- url: /testImages
  static_dir: testImages
  
- url: /stylesheets
  static_dir: stylesheets

- url: /.*
  script: index.application

libraries:
- name: webapp2
  version: "2.5.2"
- name: jinja2
  version: latest